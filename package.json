{
  "author": "Buidler Labs Developers <devs@buidlerlabs.com>",
  "description": "The Rollup plugin aiding to bundle the Hedera Strato Js library",
  "engines": {
    "node": ">=14.8"
  },
  "exports": {
    ".": {
      "require": "./lib.cjs/plugin.js",
      "import": "./lib.esm/plugin.js"
    }
  },
  "files": [
    "lib.cjs",
    "lib.esm",
    "types"
  ],
  "main": "lib.cjs/plugin.js",
  "module": "lib.esm/plugin.js",
  "name": "@buidlerlabs/rollup-plugin-hedera-strato",
  "scripts": {
    "build:4-publish": "run-p build:cjs build:esm build:declarations",
    "build:bundle-test-rollup": "rollup -c ./test/rollup.config.js",
    "build:cjs": "babel --env-name cjs lib --extensions .ts --out-dir lib.cjs",
    "build:esm": "babel --env-name esm lib --extensions .ts --out-dir lib.esm --source-maps",
    "build:declarations": "tsc",
    "clean": "run-s clean:bundle-test-rollup clean:cjs clean:esm clean:declarations",
    "clean:bundle-test-rollup": "rm -fr test/lib.esm",
    "clean:cjs": "rm -fr lib.cjs",
    "clean:declarations": "rm -fr types",
    "clean:esm": "rm -fr lib.esm",
    "lint": "eslint . --ext ts",
    "publish:npm": "run-s clean build:4-publish && npm publish --access public",
    "pretest": "run-s build:4-publish build:bundle-test-rollup",
    "test": "run-s test:in-browser",
    "test:in-browser": "karma start ./test/karma.smoke.conf.js"
  },
  "keywords": [
    "StratoJs",
    "rollup-plugin",
    "Hedera",
    "Hashgraph",
    "Smart Contracts"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "magic-string": "^0.25.7",
    "rollup": "^2.68.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "solc": "0.8.9",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.5.0",
    "events": "^3.3.0",
    "karma": "^6.3.13",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "puppeteer": "^13.1.3",
    "typescript": "^4.5.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buidler-labs/hedera-strato-rollup"
  },
  "types": "./types/plugin.d.ts",
  "version": "0.7.4",
  "peerDependencies": {
    "@buidlerlabs/hedera-strato-js": "^0.7.4"
  }
}
